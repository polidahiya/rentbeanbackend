    <script>
      window.onscroll = () => {
        document.querySelectorAll(".effect").forEach((item) => {
          let itemtop = item.getBoundingClientRect().top;
          if (itemtop < window.innerHeight - window.innerHeight / 3) {
            item.classList.add("active");
          } else {
            item.classList.remove("active");
          }
        });
      };
