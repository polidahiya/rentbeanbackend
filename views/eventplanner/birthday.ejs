    <link rel="stylesheet" href="/eventmanager/nav.css" />
    <style>
      * {
        -webkit-tap-highlight-color: transparent;
        font-family: "Mulish", sans-serif;
      }
      body {
        --theme: #e72e77;
        padding: 60px 40px;
        background-image: radial-gradient(
            circle at center center,
            rgba(217, 217, 217, 0.05) 0%,
            rgba(217, 217, 217, 0.05) 15%,
            rgba(197, 197, 197, 0.05) 15%,
            rgba(197, 197, 197, 0.05) 34%,
            rgba(178, 178, 178, 0.05) 34%,
            rgba(178, 178, 178, 0.05) 51%,
            rgba(237, 237, 237, 0.05) 51%,
            rgba(237, 237, 237, 0.05) 75%,
            rgba(138, 138, 138, 0.05) 75%,
            rgba(138, 138, 138, 0.05) 89%,
            rgba(158, 158, 158, 0.05) 89%,
            rgba(158, 158, 158, 0.05) 100%
          ),
          radial-gradient(
            circle at center center,
            rgb(255, 255, 255) 0%,
            rgb(255, 255, 255) 6%,
            rgb(255, 255, 255) 6%,
            rgb(255, 255, 255) 12%,
            rgb(255, 255, 255) 12%,
            rgb(255, 255, 255) 31%,
            rgb(255, 255, 255) 31%,
            rgb(255, 255, 255) 92%,
            rgb(255, 255, 255) 92%,
            rgb(255, 255, 255) 97%,
            rgb(255, 255, 255) 97%,
            rgb(255, 255, 255) 100%
          );
        background-size: 42px 42px;
      }
      .activebirthdaypage {
        pointer-events: all;
        opacity: 1;
      }
      ::-webkit-scrollbar {
        width: 5px;
        height: 5px;
        background-color: transparent;
      }
      ::-webkit-scrollbar-thumb {
        background-color: var(--theme);
        border-radius: 10px;
      }

      h1 {
        text-align: center;
      }
      .exiteventpage {
        position: absolute;
        top: 80px;
        left: 20px;
        background-color: var(--theme);
        color: white;
        cursor: pointer;
        border: none;
        border-radius: 50%;
        height: 35px;
        aspect-ratio: 1;
      }
      .exiteventpage:hover {
        background-color: white;
        color: var(--theme);
        outline: 1px solid var(--theme);
      }
      .exiteventpage svg {
        stroke: white;
      }
      .exiteventpage:hover svg {
        stroke: var(--theme);
      }
      section {
        box-sizing: border-box;
      }
      h2 {
        text-align: center;
        color: var(--theme);
        margin-top: 50px;
      }
      h2::before,
      h2::after {
        content: " ｡･:*˚:✧｡ ";
      }
      .scrollcontainer {
        display: flex;
        gap: 10px;
        overflow-x: scroll;
        overflow-y: hidden;
        padding: 5px 0px;
      }
      .scrollcontainer .itemcard {
        background-color: white;
        padding: 5px;
        box-sizing: border-box;
        border: 1px solid #80808033;
        border-radius: 5px;
      }
      .scrollcontainer .itemcard img {
        pointer-events: none;
        height: 250px;
      }
      .scrollcontainer .itemcard > div {
        text-align: center;
        margin: 5px 0px;
      }
      .scrollcontainer .itemcard .pricecontainer {
        box-sizing: border-box;
        display: flex;
        gap: 5px;
        font-size: 14px;
        color: var(--theme);
      }
      .scrollcontainer .itemcard .pricecontainer span:nth-child(1) {
        color: grey;
        opacity: 0.5;
        text-decoration: line-through;
        font-size: 12px;
      }
      /* magic cards */
      .gridcontainer {
        box-sizing: border-box;
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 40px;
        margin-top: 40px;
      }

      .gridcontainer .cards::before {
        content: "⭐⭐⭐";
        position: absolute;
        top: 20px;
        left: 20px;
      }
      .gridcontainer .cards {
        position: relative;
        width: 100%;
        background-color: #f8f8fa;
        border: 1px solid rgba(128, 128, 128, 0.2);
        border-radius: 10px;
        box-shadow: rgba(0, 0, 0, 0.15) 0px 0px 15px 0px;
      }
      .gridcontainer .cards > img {
        width: 100%;
        aspect-ratio: 1;
        object-fit: cover;
        border-radius: 10px 10px 0 0;
      }
      .gridcontainer .cards p {
        color: var(--theme);
        text-align: center;
        font-size: 18px;
      }
      .gridcontainer .cards p::before,
      .gridcontainer .cards p::after {
        content: " ｡･:*˚:✧｡ ";
      }
      /*  */
      .effect {
        transform: scale(0.5);
        transition: 0.6s;
        opacity: 0;
      }
      .active {
        transform: scale(1);
        opacity: 1;
      }
    </style>
  </head>
