        "/eventmanager/companies/idfc.png",
        "/eventmanager/companies/Reebok-logo.png",
        "/eventmanager/companies/The_Lalit-01.png",
      ];
      companiesarr.forEach((companie) => {
        let companiescard = document.createElement("img");
        companiescard.src = companie;
        companiescard.alt = companie;
        companiescard.setAttribute("loading", "lazy");
        document.querySelector(".companiesscroll").appendChild(companiescard);
      });
