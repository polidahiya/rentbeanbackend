    <!-- parallax -->
    <script>
      window.onscroll = () => {
        parrallax(".bgimage", 3);
        // special effect manager
        document.querySelectorAll(".eventscontainers").forEach((item) => {
          let itemtop = item.getBoundingClientRect().top;
          if (itemtop < window.innerHeight - window.innerHeight / 3) {
            item.classList.add("activeeventscontainers");
          } else {
            item.classList.remove("activeeventscontainers");
          }
        });
      };
      //
      function parrallax(elem, speed) {
        document.querySelector(elem).style.transform =
          "translateY(-" + window.scrollY / speed + "px)";
      }
    </script>
    <!-- companies -->
