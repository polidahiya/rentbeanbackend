    <%- include('footer.ejs') %>
    <!-- ui -->
    <script>
      // categories card
      fetch("data.json")
        .then((res) => res.json())
        .then((res) => {
          for (const cat in res) {
            let categorycard = document.createElement("a");
            categorycard.classList.add("card");
            categorycard.href = "categorypage?category=" + cat;
            categorycard.innerHTML = `
            <img
            src="${res[cat].image}"
            />
            <div class="title">${cat}</div>
            `;
            document
              .querySelector(".categoriescontainer")
              .appendChild(categorycard);
          }
        });
    </script>
    <!-- nav script -->
    <script src="nav.js"></script>
    <!-- moving headlines -->
    <script src="headlines.js"></script>
    <!-- pickups -->
    <script>
      // control pickups
      let pinkupleft = document.querySelector("#pinkupleft");
      let pinkupright = document.querySelector("#pinkupright");
      let cardscontainer = document.querySelector(".cardscontainer");

      pinkupleft.onclick = () => {
        cardscontainer.scrollLeft -= 400;
      };
      pinkupright.onclick = () => {
        cardscontainer.scrollLeft += 400;
      };
      //
      let pricetypename = ["mo", "day", "mo", "season"];
      fetch("data.json")
        .then((res) => res.json())
        .then((res) => {
          let productarray = [];
          for (const key in res) {
            for (const subcat in res[key]["subcat"]) {
              res[key]["subcat"][subcat].products.forEach((product) => {
                if (product.available == 0) {
                  return;
                }
                productarray.push({
                  product: product,
                  subcat: subcat,
                });
              });
            }
          }
          //
          let randomizedproducts = productarray.sort(() => Math.random() - 0.5);
          randomizedproducts.forEach((data, index) => {
            let card = document.createElement("div");
            card.classList.add("cards");
            if (index % 2 == 0) {
              card.classList.add("snap");
            }
            card.innerHTML = `
                  <a href="productpage?p=${data.product.pid}&subcat=${
              data.subcat
            }" class="imgcontainer">
                   <div class="loader">
                     <div class="dot-spinner">
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                       <div class="dot-spinner__dot"></div>
                     </div>
                   </div>
                   <img src="${data.product.image[0]}" loading="lazy" alt="" />
                     </a>
                     <a href="productpage?p=${data.product.pid}&subcat=${
              data.subcat
            }" class="name">${data.product.name}</a>
                     <div class="otherdetailscontainer">
                       <div class="rent">
                         Rent
                         <br />
                         <span>â‚¹ ${data.product.prices[0]}/${
              pricetypename[data.product.pricetype - 1]
            }</span>
                       </div>
                       <a href="subcategory?subcategory=${
                         data.subcat
                       }">See more</a>
                     </div>
                   `;
            card.querySelector(".imgcontainer img").onload = () => {
              card.querySelector(".imgcontainer .loader").style.display =
                "none";
            };
            document.querySelector(".cardscontainer").appendChild(card);
          });
        });
    </script>
    <script>
      document.body.onload = () => {
        setTimeout(() => {
          document.querySelector(".loading").style.opacity = "0";
          setTimeout(() => {
            document.querySelector(".loading").style.display = "none";
          }, 1000);
        }, 1000);
      };
    </script>
