              fill="white"
            ></path>
            <path
              d="M16.5 18.0001C17.3284 18.0001 18 18.6716 18 19.5001C18 20.3285 17.3284 21.0001 16.5 21.0001C15.6716 21.0001 15 20.3285 15 19.5001C15 18.6716 15.6716 18.0001 16.5 18.0001Z"
              fill="white"
            ></path>
          </g>
        </svg>
        <span>Cart</span>
        <div class="cartcounter">1</div>
      </a>
      <svg
        class="menu"
        onclick="showmenu()"
        viewBox="0 0 24 24"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
      >
        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
        <g
          id="SVGRepo_tracerCarrier"
          stroke-linecap="round"
          stroke-linejoin="round"
        ></g>
        <g id="SVGRepo_iconCarrier">
          <g id="Menu / Menu_Alt_05">
            <path
              id="Vector"
              d="M5 17H13M5 12H19M11 7H19"
              stroke="var(--theme)"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            ></path>
          </g>
        </g>
      </svg>
    </nav>
    <div class="desksidebar">
      <div class="locationcontainer" onclick="showlocations()">
        <img src="logo&ui/locationtheme.svg" alt="" />
        <span>Choose location</span>
      </div>
    </div>
    <div class="locationchoose activelocationchoose">
      <button onclick="hidelocations()">X</button>
      <div class="locations">Delhi</div>
      <div class="locations">Gurgaon</div>
      <div class="locations">Noida</div>
    </div>
    <div class="secnav">
      <button onclick="window.history.back()">
        <svg viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
          <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
          <g
            id="SVGRepo_tracerCarrier"
            stroke-linecap="round"
            stroke-linejoin="round"
          ></g>
          <g id="SVGRepo_iconCarrier">
            <path
              d="M7.68473 7.33186C8.07526 6.94134 8.07526 6.30817 7.68473 5.91765C7.29421 5.52712 6.66105 5.52712 6.27052 5.91765L1.60492 10.5832C0.823873 11.3643 0.823872 12.6306 1.60492 13.4117L6.27336 18.0801C6.66388 18.4706 7.29705 18.4706 7.68757 18.0801C8.0781 17.6896 8.0781 17.0564 7.68757 16.6659L4.02154 12.9998L22 12.9998C22.5523 12.9998 23 12.5521 23 11.9998C23 11.4476 22.5523 10.9998 22 10.9998L4.01675 10.9998L7.68473 7.33186Z"
              fill="rgb(82, 82, 82)"
            ></path>
          </g>
        </svg>
        <div>Back</div>
      </button>
      <span> </span>
    </div>
    <!-- product gird -->
    <div class="productgrid"></div>
    <!-- main -->
    <script>
      window.onscroll = (e) => {
        if (window.scrollY <= 10) {
          document.querySelector(".secnav").style.boxShadow = "unset";
        } else {
          document.querySelector(".secnav").style.boxShadow =
            "0 2px 10px var(--theme20)";
        }
      };
      //
      const queryString = window.location.search;
      const urlParams = new URLSearchParams(queryString);
      const product = urlParams.get("subcategory");
      document.title = product + " - RentBean";
      document.querySelector(".secnav span").innerHTML = product;
      //fetch itemlist
      let type1prices = [1, 3, 6, 12]; // month
      let type2prices = [1, 3, 7, 30]; //days
      let type3prices = [1, 3, 6]; //laptops
      let type4prices = [6]; //ac coolers
      let pricetypename=["mo","day","mo","season"]
      let producttoshow;
      fetch("data.json")
        .then((res) => res.json())
        .then((res) => {
          for (const cat in res) {
            if (!res[cat]["subcat"][product]) continue;
            res[cat]["subcat"][product]["products"].forEach((item) => {
              let card = document.createElement("a");
              card.classList.add("card");
              card.href =
                "productpage?p=" + item.pid + "&subcat=" + product;
              card.innerHTML = `
              <div class="availableornot" style="display:${item.available==1?"none":"flex"}">Currently unavailable</div>
              <div class="imgcontainer">
                <div class="loader">
                  <div class="dot-spinner">
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                    <div class="dot-spinner__dot"></div>
                  </div>
                </div>
                <img
                  src="${item.image[0]}"
                />
              </div>
              <div class="title">${item.name}</div>
              <div class="rent">Rent : â‚¹ <span>${item.prices[0]}</span>/${pricetypename[item.pricetype-1]}</div>
              `;
              card.querySelector(".imgcontainer img").onload = () => {
                card.querySelector(".imgcontainer .loader").style.display =
                  "none";
                card.querySelector(".imgcontainer img").style.opacity = 1;
              };
              document.querySelector(".productgrid").appendChild(card);
            });
          }
        });
    </script>
    <!-- nav script -->
    <script src="nav.js"></script>
  </body>
</html>
